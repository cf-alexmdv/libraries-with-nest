{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true
  },
  "projects": {
    "hello-world": {
      "type": "library",
      "root": "libs/hello-world",
      "entryFile": "index",
      "sourceRoot": "libs/hello-world/src",
      "compilerOptions": {
        "tsConfigPath": "libs/hello-world/tsconfig.lib.json"
      }
    },
    "request-decorator": {
      "type": "library",
      "root": "libs/request-decorator",
      "entryFile": "index",
      "sourceRoot": "libs/request-decorator/src",
      "compilerOptions": {
        "tsConfigPath": "libs/request-decorator/tsconfig.lib.json"
      }
    }
  }
}